<root>
<verzio>20161120</verzio>
<panel>
  <name>cikkvalpanel</name>
  <width>match</width>
  <height>match</height>
  <top>30</top>
  <visible>false</visible>
  <bgcolor>#000000</bgcolor>
  <left>0</left>
  <parent>panel1</parent>
</panel>
<text>
  <name>ebetuz</name>
  <top>0</top>
  <width>match</width>
  <parent>cikkvalpanel</parent>
  <left>0</left>
  <visible>true</visible>
  <lua_on_change>egyeb/cikkval_refresh.lua [ebetuz]</lua_on_change>
  <bgcolor>#fffffa</bgcolor>
  <font_color>#000000</font_color>
  <font_size>17</font_size>
  <height>60</height>
</text>
<table>
  <name>cikkval_table</name>
  <bgcolor>#000000</bgcolor>
  <font_color>#497000</font_color>
  <width>match</width>
  <height>600</height>
  <top>60</top>
  <left>0</left>
  <divider_color>#434343</divider_color>
  <divider_height>2</divider_height>
  
  <column_title_nev>nev</column_title_nev>
  <column_title_kod>kod</column_title_kod>
  
  <font_size>10</font_size>
  <row_height>160</row_height>
  <parent>cikkvalpanel</parent>
  <visible>true</visible>
  <itempanel>cikkval_sor</itempanel>
  <lua_after_click>egyeb/cikkval_click.lua</lua_after_click>
</table>

<panel>
    <name>cikkval_sor</name>
    <parent>cikkvalpanel</parent>
    <height>60</height>
    <width>match</width>
    <bgcolor>#497000</bgcolor>
    <visible>false</visible>
</panel>
<label>
    <name>nev</name>
    <parent>cikkval_sor</parent>
    <width>480</width>
    <height>60</height>
    <top>0</top>
    <left>0</left>
    <text>[nev]</text>
    <font_size>17</font_size>
</label>
<label>
    <name>kod</name>
    <parent>cikkval_sor</parent>
    <width>0</width>
    <height>0</height>
    <top>0</top>
    <left>0</left>
    <text>[kod]</text>
    <style>listhidden</style>
</label>

<custom_button>
  <style>buttondefault</style>
  <name>btn_folytmost2</name>
  <Text>Folyt. most</Text>
  <top>650</top>
  <width>150</width>
  <height>90</height>
  <left>0</left>
  <parent>cikkvalpanel</parent>
  <visible>true</visible>
  <lua_after_click>leltar/ujhkod.lua</lua_after_click>
</custom_button>
</root>