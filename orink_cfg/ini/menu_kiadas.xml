  <Text>Ellenőrzés</Text>
  <top>10</top>
  <width>110</width>
  <height>120</height>
  <left>135</left>
  <style>buttondefault</style>
  <visible>false</visible>
  <lua_after_click>egyeb/menuinit.lua ELLENOR [lkezelo]</lua_after_click>
</custom_button>
<custom_button>
  <name>ButtonClose</name>
  <left>135</left>
  <Text>Vissza</Text>
  <top>140</top>
  <width>110</width>
  <height>120</height>
