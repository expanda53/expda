<Root>
<verzio>20171211</verzio>
<mainpanel>
  <name>loginpanel</name>
  <text>Bejelentkezés</text>
  <bgcolor>#000000</bgcolor>
  <lua_on_create>egyeb/login_init.lua</lua_on_create>
</mainpanel>
<panel>
  <name>panel1</name>
  <width>match</width>
  <height>match</height>
  <top>0</top>
  <visible>true</visible>
  <bgcolor>#000000</bgcolor>
  <left>0</left>
</panel>
<label>
  <name>llogin</name>
  <top>0</top>
  <left>0</left>
  <width>match</width>
  <height>wrap</height>
  <visible>true</visible>
  <parent>panel1</parent>
  <style>reviewlabel</style>
  <text>Kezelő</text>
</label>
<text>
  <name>elogin</name>
  <top>30</top>
  <width>match</width>
  <parent>panel1</parent>
  <left>0</left>
  <visible>true</visible>
  <lua_on_change>egyeb/login_click.lua</lua_on_change>
  <lua_on_enter>egyeb/numeric_panel.lua show elogin egyeb/login_click.lua Bejelentkezés</lua_on_enter>
  <bgcolor>#497000</bgcolor>
  <font_color>#000000</font_color>
  <font_size>19</font_size>
  <height>40</height>
</text>


<custom_button>
  <style>buttondefault</style>
  <name>button_login</name>
  <Text>Bejelentkezés</Text>
  <top>80</top>
  <width>match</width>
  <height>70</height>
  <left>0</left>
  <lua_after_click>egyeb/login_click.lua [elogin]</lua_after_click>
  <parent>panel1</parent>
  <visible>true</visible>
  <image>arrow-256.png</image>
</custom_button>
<custom_button>
  <style>buttondefault</style>
  <name>button_exit</name>
  <Text>Kilépés</Text>
  <top>155</top>
  <width>match</width>
  <height>70</height>
  <left>0</left>
  <lua_after_click>egyeb/login_exit.lua</lua_after_click>
  <parent>panel1</parent>
  <visible>true</visible>
  <image>exit-256.png</image>
</custom_button>
<panel>
  <file>numeric_panel.xml</file>
</panel>
</Root>